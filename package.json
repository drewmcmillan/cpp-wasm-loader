  "author": "Jakub BerÃ¡nek",
  "license": "MIT",
  "version": "0.1.2",
  "keywords": [
    "wasm",
    "webpack",
    "loader",
    "c",
    "c++"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kobzol/cpp-wasm-loader.git"
  },
  "main": "dist/cjs.js",
